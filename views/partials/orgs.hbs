        <div class="logo">
            {{#if profile}}
            <img src="../images/uploads/{{profile}}" />
            {{else}}
            <img src="../images/org_user.png" alt="" />
            {{/if}}
        </div>
        <div class="header-title">

            <a href="/student-view-org/{{_id}}" id="org_name"><b>{{name}}</b></a><br />

        </div>
        <div class="actions">

            <div class="action-btn">
                
            </div>
        </div>
    </div>

</div>